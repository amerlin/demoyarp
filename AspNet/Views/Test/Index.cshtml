
@{
    ViewBag.Title = "Index";
}

<h2>Test</h2>

<div>
    <ul id="my-list"></ul>
</div>

@section scripts {

    <script>
        $(document).ready(function () {
            $.ajax({
                url: "api/values",
                contentType: "application/json",
                dataType: "json",
                success: function (data) {
                    $.each(data, function (index, value) {
                        $("#my-list").append("<li>" + value + "</li>");
                    })
                }
            });
        })
    </script>

}